[('num', Pipeline(steps=[('imputer', SimpleImputer(strategy='median')),
                ('attribs_adder',
                 addAttributes(bedrooms_ix=4, households_ix=6, population_ix=5,
                               rooms_ix=3)),
                ('std_scaler', StandardScaler())]), ['longitude', 'latitude', 'housing_median_age', 'total_rooms', 'total_bedrooms', 'population', 'households', 'median_income']), ('cat', OneHotEncoder(), ['ocean_proximity'])]